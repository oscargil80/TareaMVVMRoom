<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navgraph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.oscargil80.tareasroommvvm.UI.Fragments.HomeFragment"
        android:label="Listado de NOSe"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_createEditFragment"
            app:destination="@id/createEditFragment" />
    </fragment>
    <fragment
        android:id="@+id/createEditFragment"
        android:name="com.oscargil80.tareasroommvvm.UI.Fragments.CreateEditFragment"
        android:label="Crear Elemento "
        tools:layout="@layout/fragment_create_edit" >
        <action
            android:id="@+id/action_createEditFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>